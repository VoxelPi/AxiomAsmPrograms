# INIT TILEMAP
{
    # Init tilemap.
    R1 = 0
    {
        I1 = $EMPTY_TILEMAP
        R2 = [R1]
        I1 = $PAGE_TILEMAP
        [R1] = R2

        R1 += 1
        repeat if R1 != 0
    }
}

I1 = $PAGE_VARIABLES
[$SNAKE_HEAD_X] = 8
[$SNAKE_HEAD_Y] = 8
[$SNAKE_TAIL_X] = 6
[$SNAKE_TAIL_Y] = 8
[$SNAKE_LENGTH] = 3

I1 = $PAGE_TILEMAP
[136] = $TID_SNAKE_HEAD_R
[135] = $TID_SNAKE_X
[134] = $TID_SNAKE_TAIL_R

# UPLOAD TILEMAP
{
    write 0x02 # Send command id for texture upload.
    I1 = $PAGE_TILEMAP
    R1 = $PAGE_TILEMAP
    {
        R3 = [R1]
        write R3

        R1 += 1
        repeat if R1 != 0
    }
}

# ======================================================================================================================
# MAIN LOOP
# ======================================================================================================================
{


    # SLEEP ~ 0.5s
    {
        R7 = 0xC4
        R8 = 0x12
        {
            R7 = R7 - 1
            R8 = R8 - 0 with carry
            repeat if R8 >= 0
        }
    }
    repeat
}


