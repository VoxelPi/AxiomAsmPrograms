    {
        write 0x01 # Send command id for texture upload.
        write $TID_EMPTY # Send texture index.
        write 0x01 # Send texture config. (Use 8bit format).
        R2 = 64 # Counter
        R1 = $TEXTURE_EMPTY
        I1 = $TEXTURE_EMPTY
