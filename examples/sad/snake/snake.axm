{
    # BIT 7: COLLISION BAD
    # Bit 6: COLLISION GOOD
    # Bit 5: SNAKE

    # EMPTY:        0x00: 0000 00 00
    # WALL:         0x80: 1000 00 00

    # APPLE:        0x40: 0100 00 00
    # APPLE_GOLDEN: 0x41: 0100 00 01
    #
    # SNAKE_X:      0xA0: 1010 00 00
    # SNAKE_Y:      0xA1: 1010 00 01
    #
    # SNAKE_DR:     0xA4: 1010 01 00
    # SNAKE_DL:     0xA5: 1010 01 01
    # SNAKE_UR:     0xA6: 1010 01 10
    # SNAKE_UL:     0xA7: 1010 01 11
    #
    # SNAKE_HEAD_R: 0xA8: 1010 10 00
    # SNAKE_HEAD_U: 0xA9: 1010 10 01
    # SNAKE_HEAD_L: 0xAA: 1010 10 10
    # SNAKE_HEAD_D: 0xAB: 1010 10 11
    #
    # SNAKE_TAIL_R: 0xA8: 1010 11 00
    # SNAKE_TAIL_U: 0xA9: 1010 11 01
    # SNAKE_TAIL_L: 0xAA: 1010 11 10
    # SNAKE_TAIL_D: 0xAB: 1010 11 11

    # MEMORY:
    #   PAGE 0x00: Variables
    #       0x00: Snake head x
    #       0x01: Snake head y
    #       0x02: Snake tail x
    #       0x03: Snake tail y
    #       0x04: Snake length
    #   PAGE 0x01: Level STATE
}

$TID_EMPTY := 0x00
$TID_WALL := 0x80

$TID_APPLE := 0x40
$TID_APPLE_GOLDEN := 0x41

$TID_SNAKE_X := 0xA0
